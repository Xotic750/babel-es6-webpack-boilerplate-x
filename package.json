{
  "name": "babel-es6-webpack-boilerplate-x",
  "version": "1.1.0",
  "description": "ES6 Babel Webpack boilerplate",
  "homepage": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x/issues"
  },
  "main": "index.js",
  "scripts": {
    "prestart": "npm run production",
    "start": "nodemon --exec \"npm test\" --watch src",
    "clean": "rimraf dist",
    "production": "npm run clean && webpack --bail --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint app",
    "test": "mocha --compilers js:babel-core/register --recursive ./tests/**/*.js",
    "precommit": "npm run security && npm run lint && npm test",
    "prepush": "npm run security && npm run lint && npm test",
    "security": "nsp check"
  },
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2017",
  "license": "MIT",
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "boilerplate"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-css-modules": "^2.7.2",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsdoc": "^3.1.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-lodash": "^2.4.3",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "file-loader": "^0.11.2",
    "husky": "^0.14.1",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10",
    "npm": ">=3.10.9",
    "yarn": ">=0.21.3"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
