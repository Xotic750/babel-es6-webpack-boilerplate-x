{
  "name": "babel-es6-webpack-boilerplate-x",
  "version": "3.1.0",
  "description": "ES6 Babel Webpack boilerplate",
  "homepage": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist __tests__/coverage",
    "development": "npm run clean && NODE_ENV=development webpack --bail --config webpack.config.js --progress --profile --colors",
    "lint": "eslint -f 'node_modules/eslint-friendly-formatter' --ext .js .",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "npm run security && npm run lint && npm test",
    "prepush": "npm run precommit",
    "production": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors",
    "report": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors --env.report",
    "security": "npm audit",
    "start": "nodemon --exec \"npm run production\" --watch src",
    "test": "jest"
  },
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2017-present",
  "license": "MIT",
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "boilerplate"
  ],
  "dependencies": {
    "js-logger": "^1.4.1",
    "lodash": "^4.17.11",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
