{
  "name": "babel-es6-webpack-boilerplate-x",
  "version": "2.0.0",
  "description": "ES6 Babel Webpack boilerplate",
  "homepage": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/babel-es6-webpack-boilerplate-x/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist __tests__/coverage",
    "development": "npm run clean && NODE_ENV=development webpack --bail --config webpack.config.js --progress --profile --colors",
    "lint": "eslint --ext .js .",
    "lint-fix": "npm run clean && npm run lint -- --fix",
    "precommit": "npm run security && npm run lint && npm test",
    "prepush": "npm run precommit",
    "production": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors",
    "report": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors --env.report",
    "security": "nsp check",
    "start": "nodemon --exec \"npm run production\" --watch src",
    "test": "jest"
  },
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2017-present",
  "license": "MIT",
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "boilerplate"
  ],
  "dependencies": {
    "js-logger": "^1.4.1",
    "lodash": "^4.17.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-js-logger": "^1.0.17",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-eslint-comments": "^2.0.2",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsdoc": "^3.5.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-lodash": "^2.6.1",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-sort-class-members": "^1.3.0",
    "exports-loader": "^0.7.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "jest-file": "^1.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lodash-webpack-plugin": "^0.11.4",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.15.1",
    "nsp": "^3.2.1",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">=4.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
