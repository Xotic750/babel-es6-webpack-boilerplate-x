    "rimraf": "^2.6.2",
    "semver": "^5.5.1",
    "source-map-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
