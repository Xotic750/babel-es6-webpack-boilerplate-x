{
  "presets": [
    ["env", {
      "loose": false,
      "modules": "commonjs",
      "spec": true,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 8"
        ]
      },
      "useBuiltIns": "usage",
      "debug": false
    }],
    "stage-2"
  ],
  "plugins": [
    "dynamic-import-node",
    "js-logger",
    "lodash",
    "transform-class-properties",
    "transform-es5-property-mutators",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "transform-runtime"
  ]
}
